{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearningJenkins-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learningjenkins",
            "image": "070785328482.dkr.ecr.us-east-1.amazonaws.com/my-jenkinsapp:1.0.108",
            "portMappings": [
                {
                    "name": "ngninx-80-tip",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::070785328482:role/ecsTaskExecutionRole"
}